{
  "name": "wedding-bidders.web-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest --config projects/wedding-bidders/jest.config.js",
    "test:watch": "jest --config projects/wedding-bidders/jest.config.js --watch",
    "e2e": "npx playwright test --config projects/wedding-bidders/src/e2e/playwright.config.ts",
    "e2e:ui": "npx playwright test --config projects/wedding-bidders/src/e2e/playwright.config.ts --ui"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@10.9.4",
  "dependencies": {
    "@angular/cdk": "^21.0.5",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/material": "^21.0.5",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.4",
    "@angular/cli": "^21.0.4",
    "@angular/compiler-cli": "^21.0.0",
    "@angular/platform-browser-dynamic": "^21.0.0",
    "@playwright/test": "^1.57.0",
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "jest-preset-angular": "^16.0.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2"
  }
}
